# GoHighLevel webhook URL
# This is the URL provided by GoHighLevel when you create a webhook in your workflow
NEXT_PUBLIC_GHL_WEBHOOK_URL=your_gohighlevel_webhook_url

# Optional: API key for additional security (if needed)
# GHL_API_KEY=your_gohighlevel_api_key

# GoHighLevel API credentials
GHL_CLIENT_ID=your_gohighlevel_client_id
GHL_CLIENT_SECRET=your_gohighlevel_client_secret
GHL_REDIRECT_URI=http://localhost:3000/api/ghl-callback

# For production, use your deployed URL
# GHL_REDIRECT_URI=https://your-deployed-app.com/api/ghl-callback 